{
    "name": "networkx.graph_feature_extraction",
    "author": "Carles Sala <carles@pythiac.com>",
    "description": "Apply multiple networkx functions to extract features form a list of graphs.",
    "classifiers": {
        "type": "graph_featurization",
        "subtype": "graph_featurization"
    },
    "modalities": ["graph"],
    "primitive": "mlprimitives.utils.graph_feature_extraction",
    "produce": {
        "args": [
            {
                "name": "X",
                "type": "DataFrame"
            },
            {
                "name": "graphs",
                "type": "dict"
            }
        ],
        "output": [
            {
                "name": "X",
                "type": "DataFrame"
            }
        ]
    },
    "hyperparameters": {
        "fixed": {
            "functions": {
                "type": "list",
                "default": [
                    "networkx.degree_centrality",
                    "networkx.closeness_centrality",
                    "networkx.betweenness_centrality",
                    "networkx.clustering"
                ]
            }
        },
        "tunable": {}
    }
}
