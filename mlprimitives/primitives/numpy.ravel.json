{
    "name": "numpy.ravel",
    "contributors": [
        "Alexander Geiger <geigera@mit.edu>"
    ],
    "documentation": "https://docs.scipy.org/doc/numpy/reference/generated/numpy.ravel.html",
    "description": "Return a contiguous flattened array.",
    "classifiers": {
        "type": "helper"
    },
    "modalities": [],
    "primitive": "numpy.ravel",
    "produce": {
        "args": [
            {
                "name": "y",
                "keyword": "a",
                "type": "ndarray"
            }
        ],
        "output": [
            {
                "name": "y",
                "type": "ndarray"
            }
        ]
    },
    "hyperparameters": {
        "fixed": {
            "order": {
                "type": "str",
                "default": "C"
            }
        }
    }
}
